<core:FragmentDefinition
        xmlns="sap.m"
        xmlns:core="sap.ui.core">
    <TableSelectDialog
            id="vh-table-aor"
            noDataText="No AOR/PRE found"
            title="Select AOR/PRE"
            search=".handleSearchAor"
            confirm=".handleCloseAorVH"
            cancel=".handleCloseAorVH"
            multiSelect="false" growing="true"
            contentWidth="60rem"
            items="{aorSearch>/aorFiltered}">
        <ColumnListItem>
            <cells>
                <Text text="{aorSearch>AorNo}"/>
                <Text text="{aorSearch>AorDesc}" wrapping="true"/>
                <Text text="{aorSearch>ProjectId}" wrapping="true"/>
                <Text text="{aorSearch>FullName}" wrapping="true"/>
                <Text text="{aorSearch>Ekgrp}"/>
                <Text text="{aorSearch>Status}"/>
                <Text text="{aorSearch>AorType}"/>
            </cells>
        </ColumnListItem>
        <columns>
            <Column width="6rem">
                <Label text="AOR/PRE No."/>
            </Column>
            <Column width="15rem">
                <Label text="AOR/PRE Description"/>
            </Column>
            <Column width="10rem">
                <Label text="Project ID"/>
            </Column>
            <Column width="7rem">
                <Label text="Requestor"/>
            </Column>
            <Column>
                <Label text="Purchasing Group" tooltip="Purchasing Group"/>
            </Column>
            <Column width="6rem">
                <Label text="AOR Status"/>
            </Column>
            <Column>
                <Label text="AOR Type"/>
            </Column>
        </columns>
    </TableSelectDialog>
</core:FragmentDefinition>
