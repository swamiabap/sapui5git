<core:FragmentDefinition
        xmlns="sap.m"
        xmlns:core="sap.ui.core">
    <SelectDialog
            id="vh-dialog-costcenter2"
            title="Cost Centers" noDataText="No Cost Center found"
            class="sapUiPopupWithPadding"
            items="{costCenterSearch>/costCentersFiltered}"
            search=".handleSearch"
            confirm=".handleClose"
            cancel=".handleClose"
            contentWidth="25rem">
        <StandardListItem
                icon=""
                iconDensityAware="false"
                iconInset="false"
                title="{costCenterSearch>Description}"
                description="{costCenterSearch>CostCenter} {costCenterSearch>CompanyCode}" />
    </SelectDialog>
</core:FragmentDefinition>